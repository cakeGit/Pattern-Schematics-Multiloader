{
  "schemaVersion": 1,
  "id": "create_pattern_schematics",
  "version": "${version}",
  "name": "Create: Pattern Schematics",
  "description": "Adds repeating pattern based schematics to create!",
  "authors": [
    "Cak"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/create-pattern-schematics",
    "sources": "https://github.com/cakeGit/Pattern-Schematics-Multiloader"
  },
  "license": "MIT",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.cak.pattern_schematics.fabric.PatternSchematicsFabric"
    ],
    "client": [
      "com.cak.pattern_schematics.fabric.PatternSchematicsFabricClient"
    ]
  },
  "mixins": [
    "pattern_schematics.mixins.json",
    "pattern_schematics.common.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": ">=${create_version}"
  }
}